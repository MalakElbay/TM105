import java.util.Scanner;
public class Main {
    public static void main(String[] args) {
        Scanner input = new Scanner(System.in);
        System.out.println("Enter a number");
        int n = input.nextInt();
        int sum = 0;
        for (int i = 0 ; i <= n ; i++){
            if (i%2!=0)
                sum+=i;
        }
        System.out.printf("The sum of odd numbers= %d", sum);
    }
}
