import java.util.Scanner;
public class Main {
    public static void main(String[] args) {
        Scanner input = new Scanner(System.in);
        for (int i = 0 ; i<10 ; i++) {
            System.out.print("Enter an integer: ");
            int x = input.nextInt();
            if (x % 3 == 0 && x % 2 == 0)
                System.out.printf("%d is divisible by 2 and 3\n",x);
            else
                System.out.printf("%d is NOT divisible by 2 and 3\n",x);
        }
    }
}

