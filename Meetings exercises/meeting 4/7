import java.util.Scanner;
public class Main {
    public static void main(String[] args) {
        Scanner input = new Scanner(System.in);
        System.out.print("Enter a positive integer: ");
        int x = input.nextInt();
        int sum = 0;
        for (int i = x ; i>0 ; i/=10){
            sum+=i%10;
        }
        System.out.printf("Sum of the digits is: %d", sum);
    }
}
