import java.util.Scanner;
public class Main {
    public static String shortestString (String[] array){
        String min = array[0];
        for (int i = 0 ; i<10 ; i++){
            if(array[i].length()<min.length())
                min = array[i];
        }
        return min;
    }
    public static void main(String[] args) {
        Scanner input = new Scanner(System.in);
        String[] words = new String[10];
        System.out.print("Enter 10 words: ");
        for (int i = 0; i < 10; i++) {
            words[i] = input.next();
        }
        System.out.printf("The shortest word is: %s ", shortestString(words));
    }
}
